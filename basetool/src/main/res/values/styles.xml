<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Dialog主题,背景变暗 -->
    <style name="DialogStyle_BgDark" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!-- Dialog主题,背景不变暗 -->
    <style name="DialogStyle_BgNotDark" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <!-- AndPermission -->
    <style name="permission_PermissionActivity" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <!-- BottomNavigation -->
    <style name="BadgeText" parent="TextAppearance.AppCompat.Body2">
        <item name="android:textSize">@dimen/badge_text_size</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:maxLines">1</item>
        <item name="android:layout_marginTop">@dimen/badge_top_margin</item>
        <item name="android:paddingRight">@dimen/badge_width_padding</item>
        <item name="android:paddingLeft">@dimen/badge_width_padding</item>
        <item name="android:paddingBottom">@dimen/badge_height_padding</item>
        <item name="android:paddingTop">@dimen/badge_height_padding</item>
        <item name="android:visibility">gone</item>
    </style>

    <declare-styleable name="BottomNavigationBar">
        <attr name="bnbElevation" format="dimension" />
        <attr name="bnbActiveColor" format="color" />
        <attr name="bnbInactiveColor" format="color" />
        <attr name="bnbBackgroundColor" format="color" />
        <attr name="bnbAnimationDuration" format="integer" />
        <attr name="bnbMode" format="enum">
            <enum name="mode_default" value="0" />
            <enum name="mode_fixed" value="1" />
            <enum name="mode_shifting" value="2" />
            <enum name="mode_fixed_no_title" value="3" />
            <enum name="mode_shifting_no_title" value="4" />
        </attr>
        <attr name="bnbBackgroundStyle" format="enum">
            <enum name="background_style_default" value="0" />
            <enum name="background_style_static" value="1" />
            <enum name="background_style_ripple" value="2" />
        </attr>
        <attr name="bnbAutoHideEnabled" format="boolean" />
    </declare-styleable>

    <!-- 微信九宫格控件 -->
    <declare-styleable name="XNineGridView">
        <attr name="XNineGridView_MaxSize" format="integer" />
        <attr name="XNineGridView_Spacing" format="dimension" />
        <attr name="XNineGridView_SingleMinWidth" format="float" />
        <attr name="XNineGridView_SingleMaxWidth" format="float" />
    </declare-styleable>

    <!-- 阴影圆角Layout -->
    <declare-styleable name="RSLayout">
        <attr name="RSLayoutLeftTopRadius" format="dimension" />
        <attr name="RSLayoutLeftBottomRadius" format="dimension" />
        <attr name="RSLayoutRightTopRadius" format="dimension" />
        <attr name="RSLayoutRightBottomRadius" format="dimension" />
        <attr name="RSLayoutRadius" format="dimension" />
        <attr name="RSStrokeWidth" format="dimension" />
        <attr name="RSStrokeColor" format="color" />
        <attr name="RSShadowWidth" format="dimension" />
        <attr name="RSShadowOffsetX" format="dimension" />
        <attr name="RSShadowOffsetY" format="dimension" />
        <attr name="RSShadowColor" format="color" />
    </declare-styleable>

    <!-- 自定义可拖动进度条 -->
    <declare-styleable name="CustomSeekBar">
        <attr name="custom_seek_bar_progress_height" format="dimension" />
        <attr name="custom_seek_bar_progress_color_normal" format="color" />
        <attr name="custom_seek_bar_progress_color_select" format="color" />
        <attr name="custom_seek_bar_progress_radius" format="dimension" />
        <attr name="custom_seek_bar_circle_diameter" format="dimension" />
        <attr name="custom_seek_bar_circle_color" format="color" />
        <attr name="custom_seek_bar_circle_diameter_select" format="dimension" />
        <attr name="custom_seek_bar_circle_color_select" format="color" />
        <attr name="custom_seek_bar_ripple_diameter" format="dimension" />
        <attr name="custom_seek_bar_ripple_color" format="color" />
        <attr name="custom_seek_bar_max" format="integer" />
        <attr name="custom_seek_bar_ripple_enable" format="boolean" />
        <attr name="custom_seek_bar_progress" format="integer" />
    </declare-styleable>

    <!-- 指示器 -->
    <declare-styleable name="CommonIndicator">
        <attr name="common_indicator_select_width" format="dimension" />
        <attr name="common_indicator_unselect_width" format="dimension" />
        <attr name="common_indicator_select_color" format="color" />
        <attr name="common_indicator_unselect_color" format="color" />
        <attr name="common_indicator_padding" format="dimension" />
        <attr name="common_indicator_size" format="integer" />
    </declare-styleable>

    <!-- SmartRefresh -->
    <declare-styleable name="BallPulseFooter">
